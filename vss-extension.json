{
    "manifestVersion": 1,
    "id": "swissknife-tools",
    "version": "0.2.8",
    "name": "Swiss Knife Tools",
    "publisher": "ggarbuglia",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Swiss Knife Tools for TS and TFS",
    "icons": {
        "default": "images/SwissKnife128.png",
        "large": "images/SwissKnife512.png"
    },
    "categories": [
        "Build and release"
    ],
    "tags": [
        "utility",
        "tasks"
    ],
    "screenshots": [],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/ggarbuglia/TfsExtensions/blob/master/overview.md"
        },
        "issues": {
            "uri": "https://github.com/ggarbuglia/TfsExtensions/issues"
        },
        "repository": {
            "uri": "https://github.com/ggarbuglia/TfsExtensions"
        }
	},
    "branding": {
        "color": "black",
        "theme": "dark"
    },
    "galleryFlags": [
        "Public"
    ],
    "scopes": [
        "vso.build_execute", 
        "vso.serviceendpoint_manage" 
    ],
    "files": [
        {
            "path": "ArchiveRemoteFolder"
        },
        {
            "path": "RunSqlServerScripts"
        },
        {
            "path": "PublishToSharePoint"
        },
        {
            "path": "SetAssemblyVersion"
        },
        {
            "path": "CopyFilesViaPSSession"
        },
        {
            "path": "InternetInformationServerActions"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "archiveremotefolder-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ], 
            "properties": { 
                "name": "ArchiveRemoteFolder/Task" 
            } 
        },
        {
            "id": "runsqlserverscripts-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ], 
            "properties": { 
                "name": "RunSqlServerScripts/Task" 
            } 
        },
        {
            "id": "publishtosharepoint-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ], 
            "properties": { 
                "name": "PublishToSharePoint/Task" 
            } 
        },
        {
            "id": "setassemblyversion-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ], 
            "properties": { 
                "name": "SetAssemblyVersion/Task" 
            } 
        },
        {
            "id": "copyfilesviapssession-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ], 
            "properties": { 
                "name": "CopyFilesViaPSSession/Task" 
            } 
        },
        {
            "id": "iisactions-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ], 
            "properties": { 
                "name": "InternetInformationServerActions/Task" 
            } 
        }
    ]
}
